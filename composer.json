{
    "type": "library",
    "name": "myrostadler/proto-web",
    "license": "MIT",
    "minimum-stability": "stable",
    "prefer-stable": true,
    "require": {
        "php": ">=8.0.2",
        "ext-openssl": "*",
        "guzzlehttp/guzzle": "^7.5",
        "symfony/dotenv": "6.0.*"
    },
    "config": {
        "allow-plugins": {
            "phpstan/extension-installer": true
        },
        "optimize-autoloader": true,
        "platform": {
            "php": "8.0.2"
        },
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "MyroStadler\\ProtoWeb\\": "src",
            "Test\\MyroStadler\\ProtoWeb\\": "tests/src"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5"
    }
}
